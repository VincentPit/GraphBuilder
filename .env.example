# Example environment configuration for GraphBuilder
# Copy this file to .env and update the values

# Database Configuration
NEO4J_URI=bolt://localhost:7687
NEO4J_USER=neo4j
NEO4J_PASSWORD=your_neo4j_password
NEO4J_DATABASE=neo4j

# LLM Configuration
LLM_MODEL_NAME=azure_ai_gpt_4o

# Azure OpenAI Configuration (if using Azure)
LLM_MODEL_CONFIG_azure_ai_gpt_4o=gpt-4o,https://your-endpoint.openai.azure.com/,your-api-key,2024-02-01

# OpenAI Configuration (if using OpenAI directly)
OPENAI_API_KEY=your_openai_api_key
OPENAI_API_ENDPOINT=https://api.openai.com/v1
OPENAI_API_VERSION=2024-02-01

# Crawler Configuration
MAX_CRAWL_LIMIT=10
MAX_WORKERS=5
CRAWL_DELAY=1
ALLOWED_DOMAINS=dfrobot
VISITED_URLS_FILE=record/visited_urls.json
PROCESSED_URLS_FILE=record/processed_urls.json

# Processing Configuration
CHUNK_SIZE=200
CHUNK_OVERLAP=20
MAX_CHUNKS_ALLOWED=1000

# Logging Configuration
LOG_LEVEL=INFO
LOG_FILE=logs/graphbuilder.log